{
    "name": "provchurch/testimonies",
    "description": "A small framework to serve up testimony videos",
    "keywords": [
        "microframework",
        "rest",
        "router",
        "psr7"
    ],
    "homepage": "https://gitlab.com/nathan.rutman/testimonies",
    "license": "MIT",
    "authors": [
        {
            "name": "Nate Rutman",
            "email": "nate@provchurch.org",
            "homepage": "http://provchurch.org/"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "monolog/monolog": "^1.24",
        "php-di/php-di": "^6.0",
        "slim/psr7": "^0.5",
        "slim/slim": "^4.1",
        "slim/twig-view": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
